<h2><%= @game.team.name %> VS <%= @game.opponent %> at <%= @game.place %></h2>
<h3><%= @game.date %></h3>
<%= link_to("[Edit]", edit_game_path) %>
<ul>
<% @game.members.each do |member| %>
<li>
<% part = member.participants.find_by(game_id: @game.id) %>
<%= member.name %>(SL<%= member.skill_level %>) |<%= flag_to_mark(part.participation_flag) %>| <%= part.comment %> <%= link_to('[edit]', participants_path(part.id)) %>
</li>
<% end %>
</ul>
